{
  "$schema": "vscode://schemas/color-theme",
  "name": "Threadwork Light",
  "type": "light",
  "colors": {
    "editor.background": "#F7F4F0",
    "editor.foreground": "#3D3632",
    "editorLineNumber.foreground": "#9C9590",
    "editorLineNumber.activeForeground": "#6B6560"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#9C9590",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#3D6B4A"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#9A7B1E"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#2C3E6B"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#6B6560"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#2C3E6B"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": {
        "foreground": "#8B3A62"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#3D3632"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "support.type.property-name"],
      "settings": {
        "foreground": "#6B6560"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language"],
      "settings": {
        "foreground": "#9A7B1E"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#A63D2F"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#3D6B4A"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#6B6560"
      }
    },
    {
      "name": "Diff Added",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#3D6B4A"
      }
    },
    {
      "name": "Diff Removed",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#A63D2F"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#A63D2F"
      }
    }
  ]
}
