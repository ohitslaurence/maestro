{
  "$schema": "vscode://schemas/color-theme",
  "name": "Threadwork Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#0D0C0B",
    "editor.foreground": "#F7F4F0",
    "editorLineNumber.foreground": "#6B6560",
    "editorLineNumber.activeForeground": "#9C9590"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6B6560",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#4A7C59"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#C9A227"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#7B9BC7"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#9C9590"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#4A6FA5"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": {
        "foreground": "#8B3A62"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#F7F4F0"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "support.type.property-name"],
      "settings": {
        "foreground": "#9C9590"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language"],
      "settings": {
        "foreground": "#C9A227"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#A63D2F"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#4A7C59"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#9C9590"
      }
    },
    {
      "name": "Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#8B3A62"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#C9A227"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#F7F4F0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#8B3A62"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {
        "foreground": "#7B9BC7"
      }
    },
    {
      "name": "Module",
      "scope": ["entity.name.import", "entity.name.package"],
      "settings": {
        "foreground": "#4A7C59"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#4A6FA5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4A6FA5"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#9C9590"
      }
    },
    {
      "name": "Diff Added",
      "scope": ["markup.inserted", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#4A7C59"
      }
    },
    {
      "name": "Diff Removed",
      "scope": ["markup.deleted", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#A63D2F"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#4A6FA5"
      }
    },
    {
      "name": "YAML Key",
      "scope": ["entity.name.tag.yaml"],
      "settings": {
        "foreground": "#4A6FA5"
      }
    },
    {
      "name": "Shell Command",
      "scope": ["support.function.builtin.shell"],
      "settings": {
        "foreground": "#4A6FA5"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#A63D2F"
      }
    }
  ]
}
